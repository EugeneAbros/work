function ref()
{
	console.log('a');
	jQuery(window).trigger('resize').trigger('scroll');
}	
jQuery(window).ready(function(){
	if(window.location.pathname!="/")
	{
		
		$('.dropdown-menu a').each(function(){
			$(this).attr('href','/'+$(this).attr('href'));
		});
	}
	else
	{
		jQuery('.dropdown-menu a').bind('click', function(event) {
			var $anchor = jQuery(this);

			jQuery('html, body').stop().animate({
				scrollTop: (jQuery($anchor.attr('href')).offset().top-jQuery('.navbar').height()-50)
			}, 1500, 'easeInOutExpo');

			event.preventDefault();
		});
	}
	
	if(window.location.hash)
	{
		
		jQuery('html, body').stop().animate({
            scrollTop: (jQuery(window.location.hash).offset().top-jQuery('.navbar').height()-50)
        }, 1500, 'easeInOutExpo');
	}
	
	
	setTimeout("ref()",1000);
	
	if(jQuery('#gmap').length>0)
	{
		loadMap();
	}

	if(jQuery('#show_more').length>0) //czyli główna
	{
		jQuery('#show_more').click(function(){
			jQuery('#opinie-container').addClass('aheight')
			jQuery('#show_more').remove();
			jQuery('.overlay').remove();
		});
		
		
	}
	
	jQuery('.item a').click(function(){
		jQuery('.modal-body').html('<img src="'+jQuery(this).attr('href')+'" class="img-responsive" />');
		jQuery('#myModal').modal();
		return event.preventDefault();
	});
	new WOW().init();
});

jQuery(window).resize(function(){
	//resizeFit();	
	jQuery(window).trigger('scroll');
});


