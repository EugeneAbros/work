  elementType: "labels",
  stylers: [
    { visibility: "off" }
  ]
  },{
  featureType: "administrative",
  elementType: "labels.text.fill",
  stylers: [
    { visibility: "on" },
    { color: "#3f3f3f" }
  ]
  },{
  featureType: "road.arterial",
  elementType: "labels.icon",
  stylers: [
    { visibility: "off" }
  ]
  },{
  featureType: "water",
